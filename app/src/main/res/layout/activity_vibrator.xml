<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/activity_vibrator_textview1"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="是否有振动器" />

    <TextView
        android:id="@+id/activity_vibrator_textview2"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="短振动器" />

    <TextView
        android:id="@+id/activity_vibrator_textview3"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="长振动器" />

    <TextView
        android:id="@+id/activity_vibrator_textview4"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="节奏振动器" />

    <TextView
        android:id="@+id/activity_vibrator_textview5"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="取消振动器" />


</LinearLayout>